# https://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = crlf
insert_final_newline = true

[*.md]
indent_style = space
trim_trailing_whitespace = false

[*.cs]
indent_style = tab
trim_trailing_whitespace = true

[*.csproj]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
